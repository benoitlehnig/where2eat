<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:bb="urn:import:com.github.gwtbootstrap.client.ui.base">
	
	<g:HTMLPanel>
	    <g:HTMLPanel>
	    	<b:Heading size="6">List of Eaters </b:Heading>
	    </g:HTMLPanel>
	    <g:HTMLPanel ui:field="listOfLogins"></g:HTMLPanel>    
	    <b:ControlGroup>
             <b:Controls>
                <b:InputAddOn>
                    <b:TextBox ui:field="addLoginEmail" placeholder="Email address"/>
                    <b:Button ui:field="addLogin">Add</b:Button>
                </b:InputAddOn>
            </b:Controls>
        </b:ControlGroup>
	</g:HTMLPanel>
</ui:UiBinder> 